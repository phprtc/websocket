{
    "name": "phprtc/websocket",
    "description": "PHP RTC utility library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ahmard",
            "email": "ahmardy@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "phprtc/contracts": "dev-master",
        "phprtc/utils": "dev-master",
        "phprtc/server": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RTC\\Websocket\\": "src/",
            "RTC\\Websocket\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "phpstan analyse",
        "test": "phpunit"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5",
        "openswoole/ide-helper": "^4.10",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "config": {
        "preferred-install": "dist"
    }
}
